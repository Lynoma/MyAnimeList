<UserControl x:Class="MVVMAnimeList.Views.SearchPage"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:MVVMAnimeList.Views" xmlns:viewmodels="clr-namespace:MVVMAnimeList.ViewModels"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800">
    <Grid>
        <local:SearchPageControl Navigate="{Binding navigateDetails}" SelectionChanged="{Binding SelectionChanged}"/>
    </Grid>
</UserControl>
